    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.20.18",
    "eslint": "^8.56.0",
    "form-data": "^4.0.4",
