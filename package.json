  "keywords": [
    "fastify",
    "auth",
    "jwt",
    "google",
    "apple",
    "expo"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.4.2",
    "@fastify/env": "^4.3.0",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.3.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.12.2",
    "@fastify/swagger-ui": "^2.0.1",
    "@sinclair/typebox": "^0.31.28",
    "drizzle-orm": "^0.29.3",
    "fastify": "^4.29.1",
    "fastify-plugin": "^4.5.1",
    "file-type": "^16.5.4",
    "google-auth-library": "^10.2.0",
    "graphql": "^16.11.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "mercurius": "^13.4.1",
